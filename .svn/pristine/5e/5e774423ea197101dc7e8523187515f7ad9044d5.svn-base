package de.tum.in.net.WSNDataFramework;

/**
 * Represents an Exception thrown by the WSN.
 * Wrapper around java.lang.Exception.
 * 
 * @author Andr√© Freitag
 */
public class WSNException extends Exception {

	public WSNException() {

	}
	public WSNException(String message) {
		super(message);
	}
	public WSNException(String message, Throwable cause) {
		super(message, cause);
	}
	public WSNException(Throwable cause) {
		super(cause);
	}

	private static final long serialVersionUID = -3207118699438271861L;

}
